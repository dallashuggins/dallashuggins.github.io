{
  "name": "dallashuggins",
  "version": "1.0.0",
  "description": "Dallas Huggins website",
  "main": "index.js",
  "website": "http://dallashuggins.github.io/",
  "repository": "https://github.com/dallashuggins/dallashuggins.github.io",
  "author": "Dallas Huggins <dallas.a.huggins@gmail.com>",
  "engines": {
    "yarn": "1.13.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "react-svg-loader": "^2.1.0",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --color",
    "lint": "standard",
    "delint": "standard --fix",
    "test": "standard && jest",
    "test:watch": "jest --watch",
    "test:update": "jest --verbose -u",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-proptype-conditional-require": "^1.0.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "transform": {
      "^.+\\.(css|sass|svg)$": "<rootDir>/jest-stub.js",
      "^.+\\.js(x)?$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      ".yarn-cache"
    ],
    "modulePaths": [
      "src"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jasmine",
      "spyOn",
      "it",
      "fit",
      "expect",
      "describe",
      "fdescribe",
      "beforeEach",
      "afterEach",
      "localStorage",
      "sessionStorage",
      "location"
    ],
    "ignore": [
      "src/icons/*.js"
    ]
  }
}
